{
  "name": "booking-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "node ensure-server.js && node dist/server.js",
    "build": "rm -rf dist && tsc --project tsconfig.json",
    "dev": "ts-node-dev server/server.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/express": "^4.17.21"
  }
}
